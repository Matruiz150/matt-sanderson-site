public class Auto implements Vehiculo {
    private String marca;

    @Override
    public void acelerar() {
        System.out.println("El auto acelera");
    }

    @Override
    public void frenar() {
        System.out.println("El auto frena");
    }
}

interface Vehiculo {
    void conducir();
}

class Auto implements Vehiculo {
    public void conducir() { System.out.println("Conduciendo un auto"); }
}

class FabricaVehiculo {
    public static Vehiculo crearVehiculo() {
        return new Auto(); // Puede cambiarse por Moto, Bicicleta, etc.
    }
}


